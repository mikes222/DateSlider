<?xml version="1.0" encoding="utf-8"?>
<com.mschwartz.dateslider.SliderContainer
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:id="@+id/dateSliderContainer"
    android:layout_width="fill_parent"
    android:layout_height="fill_parent"
    style="@style/dateslidercontainer">
    <com.mschwartz.dateslider.ScrollLayout
        style="@style/Scroller"
        app:labelerClass="com.mschwartz.dateslider.labeler.HourLabeler"
        app:labelerFormat="%tk"
        app:childWidth="90dp"/>
    <com.mschwartz.dateslider.ScrollLayout
        style="@style/Scroller"
        app:labelerClass="com.mschwartz.dateslider.labeler.MinuteLabeler"
        app:labelerFormat="%tM"
        app:childWidth="45dp"/>
    <include layout="@layout/dialogbuttons"/>
</com.mschwartz.dateslider.SliderContainer>
